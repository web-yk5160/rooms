<p id="notice"><%= notice %></p>

<h1>Rooms</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th>Has daily price</th>
      <th>Daily price</th>
      <th>Has hourly price</th>
      <th>Hourly price</th>
      <th>Capacity</th>
      <th>Reputation score</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rooms.each do |room| %>
      <tr>
        <td><%= room.name %></td>
        <td><%= room.description %></td>
        <td><%= room.has_daily_price %></td>
        <td><%= room.daily_price %></td>
        <td><%= room.has_hourly_price %></td>
        <td><%= room.hourly_price %></td>
        <td><%= room.capacity %></td>
        <td><%= room.reputation_score %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>
